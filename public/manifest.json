{
  "name": "Xquare",
  "description": "Xquare extension for chromium",
  "manifest_version": 3,
  "version": "1.0.0",
  "icons": {
    "16": "imgs/Logo.png",
    "32": "imgs/Logo.png",
    "64": "imgs/Logo.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["cookies"],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}